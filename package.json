{
  "name": "the crown of life products",
  "version": "1.0.0",
  "description": "Simple e-commerce resell",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "data:import": "node server/seeder",
    "data:destroy": "node server/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "post-install": "npm install && npm install --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CNFishead/eCommerce-Page.git"
  },
  "author": "Austin Howard",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CNFishead/eCommerce-Page/issues"
  },
  "homepage": "https://github.com/CNFishead/eCommerce-Page#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "colors": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.7",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.7.2",
    "react-helmet": "^6.1.0",
    "sharp": "^0.30.1",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^2.0.7"
  }
}
